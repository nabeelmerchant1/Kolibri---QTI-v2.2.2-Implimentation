<?xml version="1.0" encoding="UTF-8"?>
<assessmentItem xmlns="http://www.imsglobal.org/xsd/imsqti_v2p2"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.imsglobal.org/xsd/imsqti_v2p2 http://www.imsglobal.org/xsd/qti/qtiv2p2/imsqti_v2p2p2.xsd"
    identifier="upload-file" title="Upload a file" adaptive="false" timeDependent="false">
    <responseDeclaration identifier="RESPONSE" cardinality="single" baseType="file"/>
    <outcomeDeclaration identifier="SCORE" cardinality="single" baseType="float" normalMaximum="20.0"/>
    <itemBody>
        <p>Upload a file here:</p>
        <uploadInteraction responseIdentifier="RESPONSE">
            <prompt>Enter the filename</prompt>
        </uploadInteraction>
    </itemBody>
</assessmentItem>
